  "main": "lib/index.cjs",
  "config": {
    "dist": "lib"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "build": "rm -rf ./%npm_package_config_dist% && rollup -c rollup.config.ts --configPlugin typescript && rm -rf ./%npm_package_config_dist%/rollup.config.d.ts ",
    "build_link": "pnpm run build && cd ./%npm_package_config_dist% && pnpm link",
    "build_publish": "pnpm run build && cd ./%npm_package_config_dist% && pnpm publish --access public",
    "publish_patch": "pnpm version patch && pnpm run build_publish",
    "publish_minor": "pnpm version minor && pnpm run build_publish",
    "publish_major": "pnpm version major && pnpm run build_publish"
  },
